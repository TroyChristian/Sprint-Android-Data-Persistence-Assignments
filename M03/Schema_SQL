CREATE TABLE player( 
p_number  INTEGER PRIMARY KEY,
first_name TEXT,
last_name TEXT,
age INTEGER,
position TEXT,
athlete_num INTEGER);
INSERT INTO player VALUES(1, "Christian", "Wilkins", 23, "Defense",1);
INSERT INTO player VALUES(2, "Andrew", "Beck", 23, "Offense", 2);
INSERT INTO player VALUES(3, "Jamal", "Adams", 23, "Defense",3);
INSERT INTO player VALUES(4, "Kirk", "Cousins", 31, "Offense",4);
INSERT INTO player VALUES(5, "EJ", "Alexander", 27, "Offense",5);

CREATE TABLE team(
  id INTEGER PRIMARY KEY,
  name TEXT,
  superbowls_won INTEGER,
  team_id INTEGER,
  FOREIGN KEY(team_id) REFERENCES player(athlete_num) );
  
